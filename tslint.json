{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "prettier": true,
    "interface-name": false,
    "import-name": false,
    "ordered-imports": false,
    "no-empty-interface": false,
    "object-literal-sort-keys": false,
    "jsx-no-lambda": false
  }
}