{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "arrow-parens": true,
    "prettier": true,
    "interface-name": false,
    "import-name": false,
    "jsx-no-lambda": false,
    "no-empty-interface": false,
    "object-literal-sort-keys": false,
    "semicolon": true,
    "trailing-comma": true
  }
}